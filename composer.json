    "keywords": [
        "encorecms",
        "dashboard"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
